<%- include header%>
<header>
    <ul>
        <li><a href="/shops">Let's Go</a></li>
        <li><a href="/">主页</a></li>
        <li class="show"><a href="/shops">商品列表</a></li>
    </ul>
    <p><a href="/cart">购物车(<span><%= total %></span>)</a></p>
</header>
   <div id="shops">
       <div class="head">商品列表</div>
       <table cellspacing="1">
           <tr>
               <td>分类</td>
               <td>名称</td>
               <td>单价(元)</td>
               <td>单位</td>
               <td></td>
           </tr>
           <% shops.forEach(function(shop,index){ %>
           <tr class="shoppings">
               <td><%= shop.category %></td>
               <td><%= shop.name %></td>
               <td><%= shop.unitPrice %></td>
               <td><%= shop.unit %></td>
               <td><a class="add" href="#">加入购物车</a></td>
           </tr>
           <% }) %>
       </table>
   </div>

<%- include footer%>